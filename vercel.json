{
  "version": 2,
  "buildCommand": "cd 21Scoutpro && npm install --production=false && npm run build",
  "outputDirectory": "21Scoutpro/dist",
  "installCommand": "cd 21Scoutpro && npm install --production=false",
  "framework": null,
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
