{
  "version": 2,
  "buildCommand": "cd backend && npm install && npm run build && cd ../21Scoutpro && npm install --production=false && npm run build",
  "outputDirectory": "21Scoutpro/dist",
  "installCommand": "cd 21Scoutpro && npm install --production=false",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
